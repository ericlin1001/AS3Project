	<script>
		function showRna() {
			seqs=document.getElementById("seqs").value;
			matches=document.getElementById("matches").value;
			//the entry point showRna
			if(!thisMovie("RNAShower").showRna(seqs,matches)){
				document.write("args error");
			}
		}
		function setWidthHeight(w,h){
		//this will be called by swf after swf.showRna
		//you can also call swf.getRealWidth(),swf.getRealHeight() and set the width,height of swf
			/**/
			thisMovie("RNAShower").width=w;
			thisMovie("RNAShower").height=h;
		//	thisMovie("RNAShower").width=thisMovie("RNAShower").getRealWidth();
		//	thisMovie("RNAShower").height=thisMovie("RNAShower").getRealHeight();
		}
		function thisMovie(movieName) {
		/*if (navigator.appName.indexOf("Microsoft") != -1) {
		return window[movieName]
		}
		else {
		return document[movieName]
		}*/
		return document.getElementById("RNAShower");
		}
